Random rand = new Random();
        int[][] Conarray = new int[5][5];
        for(int i = 0; i < 5; i++)
        {
            for(int j =0; j < 5; j++)
            {
                Conarray[i][j]=rand.nextInt(2);
                System.out.print(Conarray[i][j]);
            }
            System.out.println();
        }
        
        //This is in java.
